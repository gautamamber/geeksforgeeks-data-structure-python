    "def selection_sort(l):\n",
    "    n = len(l)\n",
    "    for i in range(n-1):\n",
    "        min_index = i\n",
    "        for j in range(i+1, n):\n",
    "            if l[i] < l[min_index]:\n",
    "                min_index = j\n",
    "            l[min_index], l[i] = l[i], l[min_index]\n",
    "    return l"
   ]
