    "def is_zero_sum(l):\n",
    "    # naive solution\n",
    "    n = len(l)\n",
    "    for i in range(n):\n",
    "        for j in range(i+1, n+1):\n",
    "            if sum(l[i:j]) == 0:\n",
    "                return True\n",
    "    return False"
   ]
  },
