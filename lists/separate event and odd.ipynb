   "source": [
    "def get_even_odd(l):\n",
    "    even = []\n",
    "    odd = []\n",
    "    for x in l:\n",
    "        if x % 2 == 0:\n",
    "            even.append(x)\n",
    "        else:\n",
    "            odd.append(x)\n",
    "    return even, odd\n"
   ]
  },
